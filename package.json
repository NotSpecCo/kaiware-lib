{
	"name": "@nothing-special/kaiware-lib",
	"version": "0.3.0",
	"type": "module",
	"author": {
		"name": "Garrett Downs",
		"url": "https://nothingspecial.io"
	},
	"homepage": "https://github.com/nothingspecialdev/kaiware-lib",
	"repository": {
		"type": "git",
		"url": "https://github.com/nothingspecialdev/kaiware-lib"
	},
	"scripts": {
		"build": "rimraf build && rollup -c --bundleConfigAsCjs",
		"format": "prettier --check .",
		"format:fix": "prettier --check --write .",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"test": "vitest",
		"test:watch": "vitest --watch",
		"test:ci": "vitest --coverage"
	},
	"files": [
		"build",
		"index.d.ts"
	],
	"types": "./build/src/index.d.ts",
	"exports": {
		".": {
			"types": "./build/esm/src/index.d.ts",
			"import": "./build/esm/src/index.js",
			"require": "./build/cjs/src/index.js"
		},
		"./lib": "./build/esm/src/lib/index.js",
		"./types": "./build/esm/src/types/index.js",
		"./enums": "./build/esm/src/enums/index.js",
		"./utils": "./build/esm/src/utils/index.js"
	},
	"devDependencies": {
		"@babel/plugin-transform-runtime": "^7.24.3",
		"@babel/preset-env": "^7.24.4",
		"@rollup/plugin-alias": "^5.1.0",
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-typescript": "^11.1.6",
		"@types/node": "^20.12.7",
		"@typescript-eslint/eslint-plugin": "^7.6.0",
		"@vitest/coverage-v8": "^1.5.0",
		"@vitest/ui": "^1.5.0",
		"eslint": "^8.57.0",
		"prettier": "^3.2.5",
		"rimraf": "^5.0.5",
		"rollup": "^4.14.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.2.8",
		"vitest": "^1.5.0"
	},
	"dependencies": {
		"@babel/runtime": "^7.24.4",
		"zod": "^3.22.4"
	}
}
